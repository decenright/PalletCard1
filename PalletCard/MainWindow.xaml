<Window x:Class="PalletCard.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PalletCard"
        mc:Ignorable="d"
        Title="MainWindow" Height="1250" Width="800">
    <DockPanel x:Name="Dockpanel" Loaded="Dockpanel_Loaded" Margin="0,0,0,-21">
        <Frame x:Name="_mainFrame" />
        <DataGrid x:Name="dataGrid" VerticalAlignment="Bottom" Width="800" Height="150" Margin="0,0,0,28"/>
    </DockPanel>
</Window>
